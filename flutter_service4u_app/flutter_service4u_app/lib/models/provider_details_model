// Provider model to hold provider details
import 'package:cloud_firestore/cloud_firestore.dart';

import 'package:cloud_firestore/cloud_firestore.dart';

class Provider {
  String name;
  String email;
  String mobile;
  String nationality;
  String major;
  String location;
  bool hasCar;
  String workingHours;

  Provider({
    required this.name,
    required this.email,
    required this.mobile,
    required this.nationality,
    required this.major,
    required this.location,
    required this.hasCar,
    required this.workingHours,
  });
}